# Copy this file to `.env` and fill the secrets. Do NOT commit the real `.env` with secrets.

# OpenAI API key for remote vision inference (optional)
OPENAI_API_KEY=

# Hugging Face token if you want to use HF remote inference
HF_TOKEN=

# Set to true to attempt automatic local model loading at startup (requires NVIDIA GPU)
AUTO_LOAD_LOCAL_MODEL=false

# Optional: enable remote inference fallback when local model isn't loaded
USE_REMOTE_INFERENCE=false
